package basics;

import static org.junit.Assert.assertEquals;

import java.util.List;

import org.junit.Test;

public class TestBinaryAddition {
	
	public void testBinaryAddition(){
		BinaryAddition ba = new BinaryAddition();
		List<Integer> number1 = ba.convertToList("11");
		List<Integer> number2 = ba.convertToList("11");
		assertEquals("111", ba.addition(number1, number2).toString());
	}

}
